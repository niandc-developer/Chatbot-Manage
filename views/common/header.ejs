<header>
	<nav class="navbar navbar-default navbar-fixed-top">
		<div class="container-fluid">
			<div class="navbar-header" style="float:left;">
				<span class="navbar-brand">管理画面</span>
			</div>
      <div class="navbar-right" style="float:right; margin-right:10px;">
				<a href="/logout" class="btn btn-default navbar-btn">ログアウト</a>
			</div>
		</div>
	</nav>
	<ol class="breadcrumb">
<% if(dir.length == 0) { %>
		<li class="active">Top</li>
<% } else {              %>
		<li><a href="/">Top</a></li>
<% }                     %>
<% for(var i=0; i < dir.length; i++) { %>
<%   if(i == dir.length-1){            %>
	<li class="active"><%= dir[i].name %></li>
<%   } else {                          %>
	<li><a href="<%= dir[i].path %>"><%= dir[i].name %></a></li>
<%   }                                 %>
<% }                                   %>
	</ol>
</header>
